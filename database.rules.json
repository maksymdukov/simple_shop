{
  "rules": {
    "orders": {
      "$order_id": {
        ".read": "root.child('/users/'+auth.uid+'/orders/'+$order_id).exists()"
      },
      ".write": true
    },
    "menu": {
      ".read": "true"
    },
    "users": {
      "$uid": {
        ".read": "$uid == auth.uid"
      },
      ".write": "auth != null"
    }
  }
}

